      "default",
      {
        "button-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "typescript-config/strict": "off",
    "button-type": "off",
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "disown-opener": "off",
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
